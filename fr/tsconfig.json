{
	"compilerOptions": {
	  "target": "es5",
	  "module": "commonjs",
	  "strict": true,
	  "esModuleInterop": true,
	  "skipLibCheck": true,
	  "forceConsistentCasingInFileNames": true,
	  "outDir": "./dist",
	  "allowJs": true,
	  "jsx": "react"
	},
	"include": ["./src/**/*"],
	"exclude": ["node_modules", "dist"]
  }